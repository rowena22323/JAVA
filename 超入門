
public class HelloWorld {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		System.out.print("Hello World");

		package koo;
		import java.io.*;
		//この場所から行末までが全てコメントです。
	public class basicgrammar {
		/*この範囲内の記述が
		コメントです。
		複数行にまたがってもOKです。 */
	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		System.out.println("初めてのプログラム");
		System.out.println("ようこそJava");
		System.out.println("頑張りましょう");
		
		//タイトル
		String title;
		title="JAVA 超入門";
				
		//価額
		int price;
		price=3000;
		
		System.out.print("本のタイトルは");
		System.out.print(title);
		System.out.println("です。");
		
		System.out.println();
		
		System.out.print("本の価額は");
		System.out.print(price);
		System.out.println("円です。");
		
		//年
		int year=1964;
		
		System.out.print("東京オリンピックは");
		System.out.print(year);
		System.out.println("年に、一回目が開催されました。");
		
		//変数に別の値を代入
		year=2020;
		System.out.print("2回目は");
		System.out.print(year);
		System.out.println("年に、開催が決定しました！");
		
		int x = 9;
		int y = 13;
		
		int z = x; //tmp선언보다 이게 더 깔끔
		x=y;
		y=z;
		
		System.out.println(x);
		System.out.println(y);
	}
}
