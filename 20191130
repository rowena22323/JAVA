package koo;
import java.io.*;
//この場所から行末までが全てコメントです。
public class basicgrammar {
/*この範囲内の記述が
コメントです。
複数行にまたがってもOKです。 */
	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		System.out.println("初めてのプログラム");
		System.out.println("ようこそJava");
		System.out.println("頑張りましょう");

	}

}

